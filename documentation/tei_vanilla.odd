            </classSpec>
          </specGrp>

          <p>The following unused model classes are removed.</p>
          <specGrp xml:id="modelclasses">
            <!-- why is this needed? the ODD processor should do it for you-->
            <classSpec type="model" ident="model.certLike" mode="delete"/>
